<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- groups -->
        <record id="module_category_insurance" model="ir.module.category">
            <field name="name">Insurance Payment System</field>
            <field name="description">Insurance Payment System</field>
            <field name="sequence">4</field>
        </record>
        <record model="res.groups" id="group_insurance_user">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_insurance"/>
            <field name="implied_ids" eval="[(4, ref('payment_jetcheckout_system.group_system_user'))]"/>
        </record>
        <record model="res.groups" id="group_insurance_manager">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_insurance"/>
            <field name="implied_ids" eval="[(4, ref('payment_jetcheckout_system.group_system_manager')), (4, ref('payment_insurance.group_insurance_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>

        <!-- rules -->
    </data>
</odoo>
